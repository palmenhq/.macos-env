alias -g doco="docker-compose"

# Jenv
export PATH="$HOME/.jenv/bin:$PATH"
eval "$(jenv init -)"

# FNM
eval "$(fnm env)" # This loads fnm (node manager)

# Go
export PATH="$PATH:$(go env GOPATH)/bin"

eval "$(thefuck --alias)"

# libpq in path
export PATH="/usr/local/opt/libpq/bin:$PATH"

# zsh theme
export PATH="/usr/local/opt/libpq/bin:$PATH"

# direnv
eval "$(direnv hook zsh)"
